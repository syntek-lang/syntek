    "lint": "eslint --ext .js,.ts --ignore-path .gitignore .",
    "unit": "mocha -r ts-node/register tests/index.test.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SebastiaanYN/Syntek.git"
  },
