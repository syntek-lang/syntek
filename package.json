  "description": "Syntek is an easy to use educational programming language",
  "main": "build/bundle.js",
  "module": "src/index.ts",
  "unpkg": "build/bundle.js",
  "scripts": {
    "test": "npm run build && npm run lint && npm run unit",
    "dev": "webpack --watch",
